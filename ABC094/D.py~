import numpy as np

n = int(input())
a=[int(i) for i in input().split()]
a=sorted(a)
max = a[-1]
del a[-1]

print(a)

ave = 0
if len(a)==1:
    print("{} {}".format(max, a[0]))
if len(a)==2:
    if(a[0]-max/2>max-a[1]):
        print("{} {}".format(max, a[0])
    else:
        print("{} {}".format(max, a[1])
else:
    print("{} {}".format(max, a[np.searchsorted(a,float(max/2))]))

    
    
